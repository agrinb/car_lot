
<div class="container">
  <h1>New Car Manufacturer:</h1>


  <%= simple_form_for @carmake, html: {class: "form-horizontal" } do |f| %>
    <%= f.input :make, label: "Car Make:", html: {class: "form-control" } %>
    <%= f.label :car_country, "Country:" %><br>
    <%= f.text_field :car_country %><br>
    <%= f.button :submit, 'Submit', class: 'btn btn-primary' %>
  <% end %>
</div>
